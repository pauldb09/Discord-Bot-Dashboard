                                <option value="<%= c.id %>" >#<%= c.name %></option>
                                <% }); %>
                                <% }else{; %>
                                    <option value="no">No set</option>
                                    <% guild.channels.filter(c => c.type === 'text').forEach(c => { %>
                                    <option value="<%= c.id %>" >#<%= c.name %></option>
                                    <% }); %>
                                    <% }; %>
                        </select>
                    </div>

                </div>
               
                <div class="input-field">
                    <div class="labele" style="text-align: left;">
