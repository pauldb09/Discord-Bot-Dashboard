                    <script>
                        const selectElement = document.querySelector('.toggle');

                        selectElement.addEventListener("change", (event) => {
                            const payload = {

                                giveaways: document.getElementById("giveaways").checked,

                                games: document.getElementById("games").checked,
                            }
                            if (payload.games) {
                                console.log("Games ok!")
                                let data = new FormData();
                                data.append('json', JSON.stringify(payload));

                                event.preventDefault();
                                fetch("/server/<%=guild.id%>/commands/enable/games", {
                                        method: 'POST',
                                        body: data
                                    })
                                    .then(response => response.json())
                                    .then(json => json)
                                    .catch(err => console.log(err));
                            } else {
                                console.log("Games not ok!")
                                let data = new FormData();
                                data.append('json', JSON.stringify(payload));

                                event.preventDefault();
                                fetch("/server/<%=guild.id%>/commands/disable/games", {
                                        method: 'POST',
                                        body: data
                                    })
                                    .then(response => response.json())
                                    .then(json => json)
                                    .catch(err => console.log(err));
                            }
                            if (payload.giveaways) {
                                console.log("Giveaways ok!")
                                let data = new FormData();
                                data.append('json', JSON.stringify(payload));

                                event.preventDefault();
                                fetch("/server/<%=guild.id%>/commands/enable/giveaways", {
                                        method: 'POST',
                                        body: data
                                    })
                                    .then(response => response.json())
                                    .then(json => json)
                                    .catch(err => console.log(err));
                            } else {
                                console.log("Games not ok!")
                                let data = new FormData();
                                data.append('json', JSON.stringify(payload));

                                event.preventDefault();
                                fetch("/server/<%=guild.id%>/commands/disable/giveaways", {
                                        method: 'POST',
                                        body: data
                                    })
                                    .then(response => response.json())
                                    .then(json => json)
                                    .catch(err => console.log(err));
                            }

                        });
                    </script>
