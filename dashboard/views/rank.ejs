        <div class="page-content">

            <h3 style="font-size: 40px;">Rank card</h3>

            <p>You can edit here your rank card. Do green rank to see your rank card.</p>
            </center>
            <br> <br>
            <form role="form" id="formgen" action="/rank" method="POST">
                <canvas height="300" width="800" id="ranks">
                    <img id="source"
       src="<%=pp%>"
       width="300" height="227">

                </canvas>


                <div class="input-field">

                    <div class="input-field">
                        <div class="labele" style="text-align: left;">
                            <label style="margin-bottom: 5px;color: #b6bef1;">
                 Background
                    </label>
                        </div>

                        <input style="width: 500px !important;background-color: #2f3136 !important;" type="color" value="#<%=color%>" name="color">
                    </div>
                </div>
                <div class="input-field">

                    <div class="input-field">
                        <div class="labele" style="text-align: left;">
                            <label style="margin-bottom: 5px;color: #b6bef1;">
                  Xp bar
                    </label>
                        </div>

                        <input style="width: 500px !important;background-color: #2f3136 !important;" type="color" value="#<%=colorxp%>" name="colorxp">
                    </div>
                </div>
                <button type="submit" class="btn" id="form_BUTTON" style="color: rgba(27, 88, 34) !important;border-color: #ced4da !important;margin-left: inherit !important;" onclick="send();">Save changes</button>
        </div>
        </div>
        </span>
        </div>
        </div>
        <footer>
            <script>
                const canvas = document.getElementById('ranks');
                const ctx = canvas.getContext('2d');

                const level = 1;
                const xp = 30;
                const xpObjectif = 60;

                const xpBarre = Math.floor(xp / xpObjectif * 510);



                ctx.font = '30px "Zen Dots"';
                ctx.fillStyle = "#fff";
                ctx.textAlign = "center";
                ctx.fillText('<%=user.username%>', 175, 290);
                ctx.font = '25px "Zen Dots"';
                ctx.fillText(`#<%=user.discriminator %>`, 175, 320);

                ctx.beginPath();
                ctx.lineWidth = 1.5;
                ctx.fillStyle = "#fff";
                ctx.moveTo(270, 175);
                ctx.quadraticCurveTo(270, 155, 300, 155);
                ctx.lineTo(750, 155);
                ctx.quadraticCurveTo(780, 155, 780, 175);
                ctx.quadraticCurveTo(780, 195, 750, 195);
                ctx.lineTo(300, 195);
                ctx.quadraticCurveTo(270, 195, 270, 175);
                ctx.fill();
                ctx.stroke();
                ctx.closePath();

                ctx.beginPath();
                ctx.lineWidth = 1.5;
                ctx.fillStyle = "#<%=colorxp%>";
                ctx.moveTo(270, 175);
                ctx.quadraticCurveTo(270, 155, 300, 155);
                ctx.lineTo(270 + xpBarre - 30, 155);
                ctx.quadraticCurveTo(270 + xpBarre, 155, 270 + xpBarre, 175);
                ctx.quadraticCurveTo(270 + xpBarre, 195, 270 + xpBarre - 30, 195);
                ctx.lineTo(300, 195);
                ctx.quadraticCurveTo(270, 195, 270, 175);
                ctx.fill();
                ctx.stroke();
                ctx.closePath();

                ctx.font = '20px "Zen Dots"';
                ctx.fillStyle = "#fff";
                ctx.textAlign = "center";
                ctx.fillText(`${xp} / ${xpObjectif}`, 525, 220)

                ctx.font = '30px "Zen Dots"';
                ctx.fillStyle = "#fff";
                ctx.textAlign = "center";
                ctx.fillText(`Level ${level}`, 420, 290)

                ctx.font = '30px "Zen Dots"';
                ctx.fillStyle = "#fff";
                ctx.textAlign = "center";
                ctx.fillText(`Rank 1`, 630, 290)

                ctx.beginPath();
                ctx.fillStyle = "#<%=color%>";
                ctx.arc(175, 175, 80, 0, Math.PI * 2);
                ctx.fill();

                ctx.beginPath();
                ctx.arc(175, 175, 75, 0, Math.PI * 2);
                ctx.closePath();
                ctx.clip();
                const avatar = document.getElementById('source');

                ctx.drawImage(avatar, 100, 100, 150, 150);
            </script>
            <script type="text/javascript">
                $('#menu-phone').on('click', function() {
                    $('.listbot').addClass('on')
                    $('.leftmenu').addClass('on')
                    $('.rightmenu').addClass('off')
                })

                $('#menu-close').on('click', function() {
                    $('.listbot').removeClass('on')
                    $('.leftmenu').removeClass('on')
                    $('.rightmenu').removeClass('off')
                })
            </script>
