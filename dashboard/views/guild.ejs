<%
const pp =  UserAvatar;
%>

    <body style="background-color: #2f313e;">
        <div class="main">
            <div class="dashboard">
                <span class="guildlist">
                    <div class="bot-list">
                    
                    <div id="783708073390112830" class="item">
                        <a href="/profile">
            <div class="bot-item tts:right" aria-label="Profil">
                <img class="icon-bot" data-id="783708073390112830" src="<%= pp %>" width="70px" height="70px">
            </div>
        </a>
    </div>
                    <hr class="bot-hr">
              
                <% guilds.forEach((guild) => { %>
                    <% if (guild.icon) { %>
                        <div id="783708073390112830" class="item">
                            <a href="/server/<%= guild.id %>">
                <div class="bot-item tts:right" aria-label="  <%= guild.name %>">
                    <img class="icon-bot" data-id="783708073390112830" src="https://cdn.discordapp.com/icons/<%= guild.id %>/<%= guild.icon %>.png" width="70px" height="70px">
                </div>
            </a>
        </div>
                        <% } else { %>

                            <div id="783708073390112830" class="item">
                                <a href="/server/<%= guild.id %>">
                    <div class="bot-item tts:right" aria-label="  <%= guild.name %>">
                        <canvas class="icon-bot" id="<%= guild.name.replace(/\w+/g, name => name[0]).replace(/\s/g, '') %>" width="70px" height="70px"></canvas>
                        <script>
                          function createIcone() {
                            const nameAcronym = document.getElementById("<%= guild.name.replace(/\w+/g, name => name[0]).replace(/\s/g, '')  %>").getContext('2d');
                            nameAcronym.font = '30px Arial';
                            nameAcronym.textAlign = "center";
                            nameAcronym.fillStyle = "white";
                            nameAcronym.fillText("<%= guild.name.replace(/\w+/g, name => name[0]).replace(/\s/g, '')  %>", 35, 45);
                          };
                          createIcone();
                        </script>
                    </div>
                </a>
            </div>

                           
                            <%}%>

                                <%});%>
                
  
                                <div class="item-leave">
                                    <div style="width: max-content;margin-left: auto;margin-right: auto;">
                                        <a href="/">
                                           </a></div></div></div></span>

                <style>
                    .leftmenu {
                        background-color: #2f3136 !important;
                    }
                </style>
                <span class="leftmenu">
                <div class="menu">
                    <div class="nav-guild">
                        <div class="forms-guild">
                            <% if (guild.icon) { %>
                                <img src="https://cdn.discordapp.com/icons/<%=guild.id%>/<%=guild.icon%>.png?size=128"    >
                            <% } else { %>
                                     <img src="https://cdn.discordapp.com/attachments/748897191879245834/782271474450825226/0.png?size=128"  >
                
                                <% } %>
                          <h4><%= guild.name %><br><br><%= guild.memberCount %> members</h4>
                        </div>
                      </div>
                       
                <div class="menu-btn">
                    <a href="/server/<%=guild.id%>">
                        <div class="btn-item btn-item-active">
                           <span class="item-span">General</span></div>
            </a>
            <a href="/server/<%=guild.id%>/welcome">
                <div class="btn-item">
                    <span class="item-span">Welcome message</span></div>
            </a>
            <a href="/server/<%=guild.id%>/leave">
                <div class="btn-item">
                    <span class="item-span">Leave messages</span></div>
            </a>
            <a href="/server/<%=guild.id%>/channels">
                <div class="btn-item">
                    <span class="item-span">Specifics channels</span></div>
            </a>

            <a href="/server/<%=guild.id%>/autorole">
                <div class="btn-item">
                    <span class="item-span">Autorole</span></div>
            </a>

            <a href="/server/<%=guild.id%>/embed">
                <div class="btn-item  ">
                    <span class="item-span">Embed sender </span></div>
            </a>
            <a href="/server/<%=guild.id%>/moderation">
                <div class="btn-item  ">
                    <span class="item-span">Boost messages </span></div>
            </a>
            <a href="/server/<%=guild.id%>/ping">
                <div class="btn-item  ">
                    <span class="item-span">Auto ping </span>
                </div>
            </a>

        </div>
        </div>
        </span>
        <span class="rightmenu">

    <div class="page-content">
      <br>
        <form role="form" id="formgen" action="/server/<%= guild.id %>" method="POST">
        
            <div class="input-field">
                <%
let prefix
if (findData) {
    prefix =findData.prefix;
} else {
    prefix = '*';
}
%>
                    <div class="labele" style="text-align: left;">
                        <label style="margin-bottom: 5px;color: #b6bef1;">
                        Prefix
                    </label>
                    </div>
                    <font style="vertical-align: inherit;">
                        <font style="vertical-align: inherit;color: #72767d;font-size:.825rem;line-height:20px;">This is the character to execute a command. Example: <%=prefix%>help  . </font>
                    </font>
                    <br>    <br>
                    <input name='new_prefix' type="text" maxlength="4" value="<%=prefix%>" required>
            </div>
            <div class="input-field">

                    <div class="labele" style="text-align: left;">
                        <label style="margin-bottom: 5px;color: #b6bef1;">
                        Embeds color   
                    </label>
                    </div>
                    <font style="vertical-align: inherit;">
                        <font style="vertical-align: inherit;color: #72767d;font-size:.825rem;line-height:20px;">It's the color of the embeds that will send Green-bot. </font>
                    </font>
                    <br>    <br>
                    <input style="width: 500px !important;background-color: #2f3136 !important;" type="color" value="<%=findData.color%>" name="embeds_color">
                  
                </div>
                
       <br>
       <div class="input-field">
        <div class="labele" style="text-align: left;">
            <label style="margin-bottom: 5px;color: #b6bef1;">
            Admin role
            </label>
        </div>
        <font style="vertical-align: inherit;">
            <font style="vertical-align: inherit;color: #72767d;font-size:.825rem;line-height:20px;">All members with this role will have acces to configuration commands without needed permissions </font>
        </font>
        <br>    <br>
        <div class="select-wrapper">

         
            <select name="admin_role" tabindex="-1">
                <% if(admin_role != null) { %>
                    <option value="<%= admin_role.id %>" >@<%= admin_role.name %></option>
                    <option value="no">No set</option>
                    <% guild.roles.filter(c => c.id !== admin_role.id && c.name !== "@everyone" && c.name !== "@here" && !c.managed).forEach(c => { %>
                    <option value="<%= c.id %>"  ><h1 style="color: black !important;">@<%= c.name %></h1></option>
        <% }); %>

            <% }else { %>
                <option value="no">No set</option>
                <% guild.roles.filter(r=>r.name !== "@everyone" && r.name !== "@here" && !r.managed).forEach(c => { %>
                    <option value="<%= c.id %>">@
                        <%= c.name %>
                    </option>
                    <% }); %>
                        <% }; %>
                            </select>
                            </div>
                            </div>

                            <br>
                            <div>
                                <div class="_3mcH5">
                                    <div class="_1g4U_">
                                        <div class="YOVBk">
                                            <font style="vertical-align: inherit;">
                                                <font style="color:#D2D3CA;vertical-align: inherit;">
                                                    Delete invocation message
                                                </font>
                                            </font>
                                        </div>
                                        <div>
                                            <div class="_2hHzh">

                                            </div>
                                        </div>
                                    </div>
                                    <div class="_3_S5z">
                                        <font style="vertical-align: inherit;">
                                            <font style="vertical-align: inherit;color: #72767d;font-size:.825rem;line-height:20px;">If you want to delete the message that calls the commands. 
                                            </font>
                                        </font>
                                        <% if(DeleteActive){  %>
                                            <label data-v-6d7a27c0="" for="delete_status" class="switch">
                                    <input data-v-6d7a27c0="" name="delete_status" id="delete_status" type="checkbox" checked> 
                                    <div data-v-6d7a27c0="" class="slider round" style="height: 30px !important;">
                                    </div>
                                </label>
                                            <% }else{%>
                                                <label data-v-6d7a27c0="" for="delete_status" class="switch">
                                        <input data-v-6d7a27c0="" name="delete_status" id="delete_status" type="checkbox" > 
                                        <div data-v-6d7a27c0="" class="slider round" style="height: 30px !important;">
                                        </div>
                                    </label>
                                                <% }%>
                                    </div>
                                </div>

                            </div>
                            <br>
                            <br>
                            <button type="submit" class="btn" id="form_BUTTON"  onclick="send();">✅ Save changes </button>

                            </form>

                            </div>
                            </span>
        </div>
        </div>
        <div style="display: none;" class="active-bot-here" id="783708073390112830"></div>
        <script type="text/javascript">
            function send() {

                document.getElementById('formgen').submit();

            }
        </script>
        <footer>
            <script type="text/javascript">
                $('#menu-phone').on('click', function() {
                    $('.listbot').addClass('on')
                    $('.leftmenu').addClass('on')
                    $('.rightmenu').addClass('off')
                })

                $('#menu-close').on('click', function() {
                    $('.listbot').removeClass('on')
                    $('.leftmenu').removeClass('on')
                    $('.rightmenu').removeClass('off')
                })
            </script>
            <script src="/responsive/jquery-3.3.1.min.js" type="text/javascript"></script>
            <script src="/responsive/jquery.ba-throttle-debounce.min.js" type="text/javascript"></script>
            <script src="/responsive/materialize.min.js" type="text/javascript"></script>
            <script src="/responsive/notyf.min.js" type="text/javascript"></script>
            <script src="/dash/jquery-3.3.1.min.js" type="text/javascript"></script>
            <script src="/dash/jquery.ba-throttle-debounce.min.js" type="text/javascript"></script>
            <script src="/dash/materialize.min.js" type="text/javascript"></script>
            <script src="/dash/notyf.min.js" type="text/javascript"></script>
            <link rel="stylesheet" href="/dash/notyf.min.css">
            <script>
                $(document).ready(() => {
                    $('select').formSelect();

                    const sortSelect = $('#sortSelect');
                    const orderSelect = $('#orderSelect');

                    if (!sortSelect || !orderSelect) return;

                    const selects = [sortSelect, orderSelect];

                    selects.forEach(s => {
                        s.change(() => {
                            window.location = s.find('option:selected').val();
                        });
                    });
                });
            </script>
        </footer>

    </body>

</html>